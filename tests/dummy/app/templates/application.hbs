<h2 id="title">Embric Demo</h2>
{{#embric-editor as |editor| }}
  <h2 class="subdued">Editor</h2>
  {{fabric-canvas width=600 height=400 editor=editor }}
  <div class="editor-controls-panel">
    <h3 class="subdued">Controls</h3>
    <div class="control-column">
      {{embric-fill-stroke-editor editor=editor}}
    </div>
    <div class="control-column">
    </div>
    <div class="control-column">
      {{embric-json-loader editor=editor json=model.startJSON}}
    </div>
  </div>
{{/embric-editor}}
{{outlet}}

